{
	"telemetry": {
		"telemetryLevel": "off"
	},

	"redhat": {
		"telemetry": {
			"enabled": false
		}
	},

	"dotnetAcquisitionExtension": {
		"telemetry": {
			"enabled": false
		}
	},

	"workbench": {
		"iconTheme": "vscode-icons",
		"editor": {
			"empty": {
				"hint": "hidden"
			}
		}
	},

	"window": {
		"title": "${activeEditorLong}${separator}${rootName}",
		"titleSeparator": " â€” "
	},

	"vsicons": {
		"dontShowNewVersionMessage": true
	},

	"locale": "ru",
	"liveServer": {
		"settings": {
			"donotShowInfoMsg": true
		}
	},

	"editor": {
		"fontSize": 18,
		"hover": {
			"delay": 1000
		}
	},

	"terminal": {
		"integrated": {
			"gpuAcceleration": "off"
		}
	},

	"markdown": {
		"preview": {
			"fontSize": 18
		}
	},

	"scm": {
		"inputFontSize": 14
	},
	"git.openRepositoryInParentFolders": "never"
}
